chart {
    id 1
    title MySQL - Aborted clients / connects
    options {
        ylabel aborts/s
        units null
        chart-type area
        series {
            name aborted_clients
            color \#ff7a0d
        }
        series {
            name aborted_connects
            color \#005467
            opposite true
        }
    }
}

chart {
    id 2
    title MySQL - Bytes received / sent
    options {
        units bytes
        ylabel bytes/s
        chart-type area
        series {
            name bytes_sent
            color \#005467
        }
        series {
            name bytes_received
            color \#ff7a0d
            opposite true
        }
    }
}

