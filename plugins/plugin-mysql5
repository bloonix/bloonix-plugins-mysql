plugin {
    id 42
    plugin MySQL5.Status
    command check-mysql5
    datatype statistic
    category Database,MySQL
    netaccess yes
    prefer localhost
    abstract MySQL check
    description MySQL innodb and myisam statistics.
}

statistic {
    statkey aborted_clients
    alias Aborted clients
    datatype float
    description See MySQL documentation, statistic variable Aborted_clients.
}

statistic {
    statkey aborted_connects
    alias Aborted connects
    datatype float
    description See MySQL documentation, statistic variable Aborted_connects.
}

statistic {
    statkey bytes_received
    alias Bytes received
    datatype float
    units bytes
    description See MySQL documentation, statistic variable Bytes_received.
}

